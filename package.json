        "@thi.ng/resolve-map": "^4.1.3",
        "@thi.ng/rstream": "latest",
        "@thi.ng/strings": "latest",
        "@thi.ng/transducers": "latest",
        "@thi.ng/transducers-hdom": "latest",
        "@thi.ng/transducers-stats": "latest"
    },
    "browserslist": [
        "last 3 Chrome versions"
    ],
    "browser": {
        "process": false
    }
